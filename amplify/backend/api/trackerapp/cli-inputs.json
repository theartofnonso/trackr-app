{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "trackerapp",
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "AMAZON_COGNITO_USER_POOLS",
      "cognitoUserPoolId": "authtrackerapp648183fb"
    },
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "AUTOMERGE"
      },
      "perModelResolutionStrategy": [
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "User"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "Routine"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "RoutineLog"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "Exercise"
        }
      ]
    }
  }
}