{
  "version": 1,
  "paths": {
    "/users/{username}": {
      "name": "/users/{username}",
      "lambdaFunction": "getUser",
      "permissions": {
        "setting": "open"
      }
    },
    "/routine-logs/{id}": {
      "name": "/routine-logs/{id}",
      "lambdaFunction": "getRoutineLog",
      "permissions": {
        "setting": "open"
      }
    },
    "/routine-templates/{id}": {
      "name": "/routine-templates/{id}",
      "lambdaFunction": "getRoutineTemplate",
      "permissions": {
        "setting": "open"
      }
    }
  }
}